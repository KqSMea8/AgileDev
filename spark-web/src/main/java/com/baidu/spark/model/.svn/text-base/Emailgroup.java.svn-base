package com.baidu.spark.model;

/**
 * 邮件组对象.
 * 
 * @author GuoLin
 *
 */
public class Emailgroup implements Emailable {

	/** 邮件组名称. */
	private String name;
	
	/** 邮件组邮件地址. */
	private String email;
	
	/**
	 * 空值构造器.
	 */
	public Emailgroup() {
		
	}

	/**
	 * 全值构造器.
	 * @param name 邮件组名称
	 * @param email 邮件组邮件地址
	 */
	public Emailgroup(String name, String email) {
		super();
		this.name = name;
		this.email = email;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}
	
}
