.tab-bg .lvl1 { margin-top: 5px; }
.tab-bg .lvl2 { margin-top: 2px; }
.tab-bg .lvl3 { margin-top: 2px; }
.tab-bg .lvl4 { margin-top: 1px; }
.tab-bg .lvl5 { margin-top: 1px; }

