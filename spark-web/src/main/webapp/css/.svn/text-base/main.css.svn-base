/** Global tag settings **/
ul, li, form, img, body, td, table, tr, th, ol, html, h1, h2, h3, h4, h5, h6, h7, p, pre { margin: 0; padding: 0; }
html { font-size: 12px; height: 90%; }
body { text-align: left; background: #fff; height: 100%; color: #000; font-family: Verdana; font-size: 12px; font-style: normal; font-variant: normal; font-weight: normal; line-height: 180%; }
img { border: none; vertical-align:middle; }
ul { list-style-type: none;}
a { color: #666; text-decoration: underline; }
a:hover { color: #666; text-decoration: underline; }
table { word-break: break-all; word-wrap: break-word; }
th, td { font-size: 12px; word-break: break-all; word-wrap: break-word; }
textarea { width: 400px; height: 120px; font-size: 12px; padding: 4px; }
input[type="text"], input[type="file"], input[type="password"], select { padding: 2px; font-family: Verdana; }
button { padding: 2px 4px; font-family: Verdana; }

/** Global class settings **/
.spark-header { zoom: 1; overflow: hidden; background: url(../images/top-banner-bg.png) repeat-x;}
.spark-header .logo { background: url(../images/top-banner-logo.jpg) no-repeat; height: 65px; margin:8px 0 0 8px; cursor: pointer; width: 350px;}
.spark-header .location {  }
.spark-header .tabs { background:#69a8de;height:34px; margin: 0 0 0 8px;}
.spark-header .tabs a { padding:0 40px; color:#fff; line-height:34px; text-decoration:none; float:left; font-size:18px; font-family:arial; border-right:1px solid #9ec8f3;}
.spark-header .tabs a.first { background:url(../images/nav_first.gif) no-repeat; width:165px; padding:0; text-align:center;}
.spark-header .tabs a.selected { background:#2f85cf; padding:0 40px; text-align:center; }
.spark-header .user-info { margin-right:12px; float :right ;line-height:26px;}
.spark-header .backward { }
.spark-topbar { zoom: 1; overflow: hidden;} 
.spark-body { margin: 0 0 0 8px; }
.spark-main { margin: 4px 0 0 0;}
.spark-footer { }

.tab { overflow: hidden; zoom: 1; }
.tab-bg { height: 100%; float: left; }
.tab-inner-wrapper { height: 100%; float: left; padding: 0 8px; }
