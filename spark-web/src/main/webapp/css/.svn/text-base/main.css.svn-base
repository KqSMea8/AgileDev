/** CSS for rich-content from text editor*/
.rich-content {}
.rich-content p { display: block; margin: 1em 0; }
.rich-content ol { display: block;  list-style-type:decimal; margin: 1em 0; -moz-padding-start: 40px; padding: 0 0 0 30px;}
.rich-content ul { display: block; list-style-type:disc; margin: 1em 0; -moz-padding-start: 40px; padding: 0 0 0 30px;}
.rich-content li { display: list-item;}
/* 2 deep unordered lists use a circle */
.rich-content ol ul, .rich-content ul ul { list-style-type: circle;}
/* 3 deep (or more) unordered lists use a square */
.rich-content ol ol ul, .rich-content  ol ul ul { list-style-type: square;}


