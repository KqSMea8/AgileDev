.warning { background : #fffaa0; color : #860; border: 1px solid #e0d090; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; padding: 8px 12px; margin: 8px 0; }

.space-at-left { margin : 0 0 0 10px }
.left { float:left;}
.right { float:right;}
.clear-both { clear:both; font-size:0; line-height:0; }
.hide { display: none; }
