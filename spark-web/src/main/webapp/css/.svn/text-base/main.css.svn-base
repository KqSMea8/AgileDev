h3 { font-size: 14px; font-weight: bold; color: #333; }
.align-center { text-align: center; }
.wordwrap { word-wrap: break-word; word-break: break-all; }
.blank { background: #fff; }

button.text { margin: 0; padding: 0; border: none; background: none; cursor: pointer }
button.link { margin: 0; padding: 0; border: none; background: none; cursor: pointer; text-decoration: underline; }
button.simple { border: none; cursor: pointer; margin: 0; padding: 0; }
.button-create {}
.button-import {}
.button-back {}
