.warning-message .warning-message-icon{ background: url("../images/warning.png") no-repeat left 4px; width: 20px; height: 20px; display: block; float: left;}
.info-message { margin: 12px 0; background: #fff9e5; padding: 12px; border: 1px solid #EEB420; border-radius: 4px; -webkit-border-radius: 4px; -moz-border-radius: 4px; }
