.button-collapse { width: 16px; height: 16px; background: url(cupertino/images/ui-icons_888888_256x240.png) -64px -18px no-repeat }

.radius-small {-webkit-border-radius: 4px;-moz-border-radius: 4px; border-radius: 4px;}
.radius-large {-webkit-border-radius: 8px;-moz-border-radius: 8px; border-radius: 8px;}
.radius-small-top {-webkit-border-top-left-radius: 4px;-webkit-border-top-right-radius: 4px;-moz-border-radius-topleft: 4px;-moz-border-radius-topright: 4px;border-top-left-radius: 4px;border-top-right-radius: 4px;}
.radius-large-top {-webkit-border-top-left-radius: 8px;-webkit-border-top-right-radius: 8px;-moz-border-radius-topleft: 8px;-moz-border-radius-topright: 8px;border-top-left-radius: 8px;border-top-right-radius: 8px;}
.radius-large-left {-webkit-border-top-left-radius: 8px;-webkit-border-bottom-left-radius: 8px;-moz-border-radius-topleft: 8px;-moz-border-radius-bottomleft: 8px;border-top-left-radius: 8px;border-bottom-left-radius: 8px;}

/** Bussiness class settings */
.list-table { width: 100%; border-collapse: collapse; }
.list-table th, .list-table td { line-height: 150%; border: 1px solid #e7e7e7; }
.list-table th { text-align: center; color: #444; background: #f2f2f2; padding: 2px 8px 4px; white-space:nowrap; }
.list-table td { padding: 4px 8px; }
.list-table .col-20 { width: 20%; }
.list-table .col-30 { width: 30%; }
.list-table .col-12 { width: 12%; }
.list-table .col-first, .list-table td:nth-child(1), .list-table th:nth-child(1) { border-left: none; }
.list-table .col-last, .list-table td:last-child, .list-table th:last-child { border-right: none; }
.list-table .col-action { text-align: right; width: 12%; }
.list-table .col-action a { margin-left: 8px; }
.list-table .row-first {}
.list-table .row-last {}
.list-table .odd td, .list-table tr:nth-child(2n+1) td {  }
.list-table .even td, .list-table tr:nth-child(2n) td { background: #f5f7f9; }
.list-table tr:hover td { background: #e0f0ff; }

.details-table { width: 100%; border-collapse: collapse; }
.details-table th, .details-table td { padding: 4px 8px; line-height: 150%; border: 1px solid #e7e7e7; }
.details-table th { text-align: right; width: 14%; color: #444; background: #f5f7f9; }
.details-table td { }
.details-table .col-first, .details-table td:nth-child(1), .details-table th:nth-child(1) { border-left: none; }
.details-table .col-last, .details-table td:last-child, .details-table th:last-child { border-right: none; }
.details-table .row-first {}
.details-table .row-last {}
/**.details-table tr:hover td, .details-table tr:hover th { background: #e0f0ff; }*/

.card-list { overflow:hidden; zoom: 1;}
.list { width: 100%; border-collapse: collapse;}
.list th, .list td { line-height: 150%; border: 1px solid #e5e5e5;}
.list th { text-align: center; color: #444; padding: 2px 8px 4px; word-wrap: break-word; word-break: break-all; overflow: hidden;}
.list td { padding: 4px 8px; }
.list th a { text-decoration: none;}
.list thead th, .list thead th span { background: url("cupertino/images/ui-bg_glass_60_eeeeee_1x400.png") repeat-x scroll 50% 50% #eeeeee;}
.list tbody tr:hover td { background: #e0f0ff; }
.list tr.list-header td{ background-color: #f2f5f7; border:none; border-bottom:1px solid #BBBBBB; }
