.list th:hover, .list th:hover span{background: url("cupertino/images/ui-bg_highlight-hard_100_f2f5f7_1x100.png") repeat-x scroll 50% 50% #DEEDF7;}
.list td.checkbox { padding:0px; width:24px; background-color: #f8f8f8; border: 1px solid white;}
