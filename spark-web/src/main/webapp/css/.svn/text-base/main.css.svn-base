.list th.colorful { padding:0px;}
.list td.sequence { width: 70px; color:#999; }
.list td.card-title a { text-decoration: none }
.list td.card-title a:hover { text-decoration: underline; }

.export-excel-icon {background: url(cupertino/images/ui-icons_3d80b3_256x240.png) -112px -112px no-repeat; width: 16px; height: 16px; position: absolute; display: block; }
.select-columns-icon {background: url(cupertino/images/ui-icons_3d80b3_256x240.png) -192px -112px no-repeat; width: 16px; height: 16px; position: absolute; display: block; }

.attachment-icon { background: url("../images/attachment-col.png") 4px 4px no-repeat; width: 16px; height: 16px; }
.icon-link { color: #2779AA; margin-left:16px;}

.hierarchy {line-height: 125%; }
.hierarchy ul {}
.hierarchy ul li {  }
.hierarchy li ul.children { margin-left: 16px; }
.hierarchy .item { height: 35px; zoom: 1; overflow: visible; padding: 0; border: 1px solid #ddd; margin: 3px 0; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px }
.hierarchy .expand { background: url(cupertino/images/ui-icons_888888_256x240.png) -32px -16px no-repeat; padding-left: 16px; cursor: pointer; }
.hierarchy .collapse { background: url(cupertino/images/ui-icons_888888_256x240.png) -64px -16px no-repeat; padding-left: 16px; cursor: pointer; }
.hierarchy .selectable { float: left; width: 24px; height: 100%; margin: 0 -6px 0 6px; _margin: 0 -6px 0 3px; /* IE6 Support */ background: #fff; }
.hierarchy .item-head { overflow: hidden; height: 100%; width: 101px; _width: 104px; _margin-right: -3px; /* IE6 Support */ float: left; background: #fff; border-right: 1px solid #ddd; }
.hierarchy .item-head h4 { font-size: 12px; font-weight: bold; margin: 2px 8px; word-wrap: break-word; overflow: hidden; zoom:1 }
.hierarchy .item-head h4 a, .hierarchy .item-head h4 span { float:left } 
.hierarchy .item-head button {  }
.hierarchy .item-head span { color: #999; text-decoration: none;}
.hierarchy .item-body { height: 100%; margin: 0 150px 0 125px; background: #f8f8f8; padding: 0 2px; }
.hierarchy .item-body ul { overflow: hidden; zoom: 1; width: 100%; height: 100%; }
.hierarchy .item-body ul li { display: table; float: left; height: 100%; word-wrap: break-word; word-break: break-all; overflow: hidden; }
.hierarchy .item-body ul li div { padding: 2px 4px; display: table-cell; vertical-align: middle; }
.hierarchy .item-body li.title { font-size: 12px; font-weight: normal; text-align: left; width: 200px;}
.hierarchy .item-body li.title a { text-decoration: none; }
.hierarchy .item-body li.title a:hover { text-decoration: underline; }
.hierarchy .item-body li.column { border-left: 1px solid #e5e5e5; text-align: center; }
.hierarchy .operations { float: right; width: 150px; _width: 153px; _margin-left: -3px; /* IE6 Support */ height: 100%; border-left: 1px solid #e5e5e5; background: #f8f8f8; }
.hierarchy .operations .operation { float: left; margin: 9px 4px; }
.hierarchy .operations .operation .icon-link {color: #2779AA; margin-left:16px; text-decoration: none; }
.hierarchy .operations .edit-icon { position: absolute; width: 16px; height: 16px; display: block; background: url(cupertino/images/ui-icons_3d80b3_256x240.png) -64px -112px no-repeat; }
.hierarchy .operations .add-icon { position: absolute; width: 16px; height: 16px; display: block; background: url(cupertino/images/ui-icons_3d80b3_256x240.png) -16px -128px no-repeat; }
.hierarchy .operations .delete-icon { position: absolute; width: 16px; height: 16px; display: block; background: url(cupertino/images/ui-icons_3d80b3_256x240.png) -80px -128px; no-repeat; }

.hierarchy .hierarchy-header .item { height: auto !important; overflow: visible; border-color: #fff; height: 20px; text-align: center;}
.hierarchy .hierarchy-header .item-head { border-color: #fff; background: none; }
.hierarchy .hierarchy-header .item-body { border-color: #eee; font-weight: bold; background: none; }
.hierarchy .hierarchy-header .item-body ul { _overflow: visible; /* IE6 Support */ }
.hierarchy .hierarchy-header .item-body ul li { overflow: visible; }
.hierarchy .hierarchy-header .item-body ul li.title { font-weight: bold; text-align: center; }

